    "    'model__alpha': (0.01,0.1,1,10)\n",
    "}\n",
    "\n",
    "\n",
    "# Perform grid search on pipeline\n",
    "\n",
    "gs_bag = GridSearchCV(pipe, params, n_jobs=-1, scoring='accuracy', cv=5)\n",
    "\n",
    "gs_bag.fit(data.clean_reviews, data.target)"
